      "PySCFString += f'orbital = mc_sa.mo_coeff\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'# SS-CASSCF wavefunction.\\n'\n",
      "PySCFString += f'mc_ss = mcscf.CASCI(mf, norbs, nelec).state_specific_(state)\\n'\n",
      "PySCFString += f'mc_ss.kernel(orbital)\\n'\n",
      "PySCFString += f'ss_state = mc_ss.ci\\n'\n",
      "PySCFString += f'ss_e = mc_ss.e_tot\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'# CASCI to get higher roots\\n'\n",
      "PySCFString += f'mc_es = mcscf.CASCI(mf, norbs, nelec)\\n'\n",
      "PySCFString += f'mc_es.fcisolver.nroots = nstates\\n'\n",
      "PySCFString += f'mc_es.conv_tol  = sp_conv_e\\n'\n",
      "PySCFString += f'mc_es.fix_spin_(ss=spin)\\n'\n",
      "PySCFString += f'mc_es.kernel(orbital)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'eList = mc_es.e_tot\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'#NEVPT2 for each root\\n'\n",
      "PySCFString += f'nevptEnergy = []\\n'\n",
      "PySCFString += f'for root in range(nstates):\\n'\n",
      "PySCFString += f'    nevptEnergy += [mrpt.NEVPT(mc_es, root=root).kernel()]\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'# transition density matrix and transition dipole\\n'\n",
      "PySCFString += f'charges = new_mol.atom_charges()\\n'\n",
      "PySCFString += f'coords = new_mol.atom_coords()\\n'\n",
      "PySCFString += f'nuc_charge_center = np.einsum(\"z,zx->x\", charges, coords) / charges.sum()\\n'\n",
      "PySCFString += f'new_mol.set_common_orig_(nuc_charge_center)\\n'\n",
      "PySCFString += f'dip_ints = new_mol.intor(\"cint1e_r_sph\", comp=3)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'resetData()\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'writeData(\"\\n------------------------------------------------------------------------\")\\n'\n",
