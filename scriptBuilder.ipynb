      "PySCFString += f'\\n'\n",
      "PySCFString += f'# SA-CASSCF to get optimal orbitals\\n'\n",
      "PySCFString += f'mc_sa = new_mol.CASSCF(norbs, nelec).state_average(weights_sa)\\n'\n",
      "PySCFString += f'mc_sa.max_cycle_macro = 500\\n'\n",
      "PySCFString += f'mc_sa.max_cycle_micro = 20\\n'\n",
