     "text": [
      "PySCFString = ''\n",
      "PySCFString += f'from pyscf import gto, lib, mrpt, mcscf\\n'\n",
      "PySCFString += f'import numpy as np\\n'\n",
      "PySCFString += f'import matplotlib.pyplot as plt\\n'\n",
      "PySCFString += f'from matplotlib.colors import hsv_to_rgb\\n'\n",
      "PySCFString += f'from functools import reduce\\n'\n",
      "PySCFString += f'import os\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'lib.num_threads(16)\\n'\n",
      "PySCFString += f'jobName = os.environ[\"SLURM_JOB_NAME\"].split(\".\")[0]\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def evToNm(eV: float|list[float], error: float=0.0) -> float|tuple[float, float, float]:\\n'\n",
      "PySCFString += f'    def ev2nm(eV):\\n'\n",
      "PySCFString += f'        h = 4.135667e-15\\n'\n",
      "PySCFString += f'        c = 2.99792e8\\n'\n",
      "PySCFString += f'        return np.divide(np.multiply(h,c), np.multiply(eV,1e-9))\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    if type(eV) == float:\\n'\n",
      "PySCFString += f'        nm = ev2nm([eV, eV+error, eV-error])\\n'\n",
      "PySCFString += f'        if   error == 0:\\n'\n",
      "PySCFString += f'            return nm[0]\\n'\n",
      "PySCFString += f'        elif error != 0:\\n'\n",
      "PySCFString += f'            return (nm[0], nm[1], nm[2])\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    else:\\n'\n",
      "PySCFString += f'        nm = ev2nm(eV)\\n'\n",
      "PySCFString += f'        return nm\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def printXYZ(mol):\\n'\n",
      "PySCFString += f'    coords = f\"{mol.natm}\\n\\n\"\\n'\n",
      "PySCFString += f'    for i in range(mol.natm):\\n'\n",
      "PySCFString += f'        x, y, z = mol.atom_coord(i)\\n'\n",
      "PySCFString += f'        coords += f\"{mol.atom_symbol(i)}    {x*0.529177: .10f}    {y*0.529177: .10f}    {z*0.529177: .10f}\\n\"\\n'\n",
      "PySCFString += f'    print(coords)\\n'\n",
      "PySCFString += f'    return coords\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def m_diag(occ_ref:list[float]|np.ndarray, occ_no:list[float]|np.ndarray) -> float:\\n'\n",
      "PySCFString += f'    if type(occ_ref) == list: occ_ref = np.array(occ_ref)\\n'\n",
      "PySCFString += f'    if type(occ_no) == list: occ_no = np.array(occ_no)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    docc = occ_no[np.equal(occ_ref, 2.)].tolist()\\n'\n",
      "PySCFString += f'    socc = occ_no[np.equal(occ_ref, 1.)].tolist()\\n'\n",
      "PySCFString += f'    uocc = occ_no[np.equal(occ_ref, 0.)].tolist()\\n'\n",
      "PySCFString += f'    soccm1 = np.abs(np.subtract(socc, 1))\\n'\n",
      "PySCFString += f'    m = 0.5*(2-min(docc)+np.sum(soccm1)+max(uocc))\\n'\n",
      "PySCFString += f'    return round(m, 3)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def plot(mc, degeneracyTol:float=0.01, \\n'\n",
      "PySCFString += f'             annotateSize:int=6, \\n'\n",
      "PySCFString += f'             annotateOffset:float=0.5,\\n'\n",
      "PySCFString += f'             figSize:list[int,int]=[10, 5],\\n'\n",
      "PySCFString += f'             ymax:list[int,int]=[-2, 2]) -> plt.figure:\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    def makeDegeneracyList(orbitals:list[float], degeneracyTol:float) -> np.ndarray:\\n'\n",
      "PySCFString += f'        currentBin = []\\n'\n",
      "PySCFString += f'        bins = []\\n'\n",
      "PySCFString += f'        binNum = 0\\n'\n",
      "PySCFString += f'        maxDegeneracy = 0\\n'\n",
      "PySCFString += f'        for i in range(len(orbitals)):\\n'\n",
      "PySCFString += f'            if i == 0:\\n'\n",
      "PySCFString += f'                currentBin += [binNum] # set our intial bin number\\n'\n",
      "PySCFString += f'                binNum += 1\\n'\n",
      "PySCFString += f'            else:\\n'\n",
      "PySCFString += f'                if abs(orbitals[i] - orbitals[i-1]) <= degeneracyTol: # if the energy difference between this and the previous orbital is\\n'\n",
      "PySCFString += f'                    currentBin += [binNum] #                             witihin the degenracy tolerance, append to the current bin\\n'\n",
      "PySCFString += f'                    binNum += 1\\n'\n",
      "PySCFString += f'                else:\\n'\n",
      "PySCFString += f'                    bins += [currentBin]              # otherwise, append the previous bin to the bin list \\n'\n",
      "PySCFString += f'                    binNum = 0\\n'\n",
      "PySCFString += f'                    currentBin = [binNum]             # create a new bin with the new bin number\\n'\n",
      "PySCFString += f'                    binNum += 1\\n'\n",
      "PySCFString += f'                    if len(bins[-1]) > maxDegeneracy: # update the max bin size\\n'\n",
      "PySCFString += f'                        maxDegeneracy = len(bins[-1])\\n'\n",
      "PySCFString += f'        bins += [currentBin]\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'        if len(bins[-1]) > maxDegeneracy:             # in case the last bin was degenerate, we want to update the bin size anyway\\n'\n",
      "PySCFString += f'            maxDegeneracy = len(bins[-1])\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'        offsetBins = []\\n'\n",
      "PySCFString += f'        for i in bins:\\n'\n",
      "PySCFString += f'            if len(i)%2 != 0:\\n'\n",
      "PySCFString += f'                if len(i) != 1:\\n'\n",
      "PySCFString += f'                    newbin = np.subtract(i, (len(i)-1)/2)\\n'\n",
      "PySCFString += f'                else:\\n'\n",
      "PySCFString += f'                    newbin = i\\n'\n",
      "PySCFString += f'            else:\\n'\n",
      "PySCFString += f'                newbin = np.subtract(i, (len(i)-1)/2)\\n'\n",
      "PySCFString += f'                \\n'\n",
      "PySCFString += f'            offsetBins = np.append(offsetBins, newbin)\\n'\n",
      "PySCFString += f'        return offsetBins\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    figH, figW = figSize\\n'\n",
      "PySCFString += f'    ymin, ymax = ymax\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    fig, ax = plt.subplots(1,1, figsize=(figW,figH))\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    orbitals = mc.mo_energy.tolist()\\n'\n",
      "PySCFString += f'    occupation = np.round(mc.mo_occ, 3).tolist()\\n'\n",
      "PySCFString += f'    occupationCol = np.divide(np.add(np.multiply(np.divide(occupation, 2), 120), 240), 360)\\n'\n",
      "PySCFString += f'    occupationColOut = []\\n'\n",
      "PySCFString += f'    for i, val in enumerate(occupationCol):\\n'\n",
      "PySCFString += f'        r, g, b = hsv_to_rgb((val, 1, 1))\\n'\n",
      "PySCFString += f'        occupationColOut += [(r, g, b)]\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    occupationCol = occupationColOut\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    labels = [count for count, i in enumerate(orbitals)]\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    degen = makeDegeneracyList(orbitals, degeneracyTol)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    ax.scatter(degen, orbitals, marker=\"_\", s=2000, color=occupationCol)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    for i, val in enumerate(labels):\\n'\n",
      "PySCFString += f'        ax.annotate(val, (degen[i]+annotateOffset, orbitals[i]), fontsize=annotateSize)\\n'\n",
      "PySCFString += f'    for i, val in enumerate(occupation):\\n'\n",
      "PySCFString += f'        if val < 1.999 and val > 0.001:\\n'\n",
      "PySCFString += f'            if val >= 1.95 or val <=0.05:\\n'\n",
      "PySCFString += f'                ax.annotate(val, (degen[i]-annotateOffset*2, orbitals[i]), fontsize=annotateSize, weight=\"bold\", bbox=dict(boxstyle=\"square,pad=0.3\", fc=\"white\", ec=\"magenta\", lw=2))\\n'\n",
      "PySCFString += f'            else:\\n'\n",
      "PySCFString += f'                ax.annotate(val, (degen[i]-annotateOffset*2, orbitals[i]), fontsize=annotateSize, weight=\"bold\", bbox=dict(boxstyle=\"square,pad=0.3\", fc=\"white\", ec=\"green\", lw=2))\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'    ax.set_ylim((ymin, ymax))\\n'\n",
      "PySCFString += f'    ax.set_xlim((min(degen-0.5), max(degen+0.5)))\\n'\n",
      "PySCFString += f'    ax.set_ylabel(r\"Energy ($eV$)\")\\n'\n",
      "PySCFString += f'    ax.set_xticks([], [])\\n'\n",
      "PySCFString += f'    plt.close()\\n'\n",
      "PySCFString += f'    return fig\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def makedip(ci_id):\\n'\n",
      "PySCFString += f'    # transform density matrix in MO representation\\n'\n",
      "PySCFString += f'    t_dm1 = mc_es.fcisolver.trans_rdm1(ss_state, mc_es.ci[ci_id], mc_es.ncas, mc_es.nelecas)\\n'\n",
      "PySCFString += f'    # transform density matrix to AO representation\\n'\n",
      "PySCFString += f'    orbcas = orbital[:,mc_es.ncore:mc_es.ncore+mc_es.ncas]\\n'\n",
      "PySCFString += f'    t_dm1_ao = reduce(np.dot, (orbcas, t_dm1, orbcas.T))\\n'\n",
      "PySCFString += f'    # transition dipoles\\n'\n",
      "PySCFString += f'    return np.einsum(\"xij,ji->x\", dip_ints, t_dm1_ao)\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def writeData(string:str) -> None:\\n'\n",
      "PySCFString += f'    with open(f\"{jobName}-data.out\", \"a\") as f:\\n'\n",
      "PySCFString += f'        f.write(f\"{string}\\n\")\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'def resetData() -> None:\\n'\n",
      "PySCFString += f'    with open(f\"{jobName}-data.out\", \"w+\") as f:\\n'\n",
      "PySCFString += f'        f.write(\"\")\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'mol = gto.Mole()\\n'\n",
      "PySCFString += f'mol.atom = \"\"\" \\n'\n",
      "PySCFString += f'C     1.9512287631    -1.2953096906    -0.0000364829\\n'\n",
      "PySCFString += f'C     2.5768146966     0.0224181852    -0.0000008653\\n'\n",
      "PySCFString += f'C     2.0067248856     1.2792302433     0.0000356191\\n'\n",
      "PySCFString += f'C     0.5747108866     1.6518430617     0.0000491682\\n'\n",
      "PySCFString += f'C    -0.5714696207     0.7378279492     0.0000261439\\n'\n",
