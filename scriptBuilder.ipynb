      "PySCFString += f'        writeData(f\"0 -> {i:<3}     {0.0:4.6f}    {0.0:4.6f}      {0.0:>7.2f}      {0.0:.6f}\")\\n'\n",
      "PySCFString += f'    else:\\n'\n",
      "PySCFString += f'        eEh = abs(mc_es.e_tot[i] - mc_es.e_tot[state])\\n'\n",
      "PySCFString += f'        eEh = eEh\\n'\n",
      "PySCFString += f'        eeV = eEh * 27.2114\\n'\n",
      "PySCFString += f'        enm = evToNm(eeV)\\n'\n",
      "PySCFString += f'        dipole = np.linalg.norm(np.square(makedip(i)))\\n'\n",
      "PySCFString += f'        writeData(f\"0 -> {i:<3}     {eEh:4.6f}    {eeV:4.6f}      {enm:>7.2f}      {dipole:.6f}\")\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'writeData(\"\\n\\n------------------------------------------------------------------------\")\\n'\n",
      "PySCFString += f'writeData(\"------------------------------- NEVPT2 ---------------------------------\")\\n'\n",
      "PySCFString += f'writeData(\"------------------------------------------------------------------------\")\\n'\n",
      "PySCFString += f'writeData(\"\\n\")\\n'\n",
      "PySCFString += f'writeData(\"Transition      E(Eh)      E (eV)      E(nm)    |Transition dipole|^2   \")\\n'\n",
      "PySCFString += f'writeData(\"------------------------------------------------------------------------\")\\n'\n",
      "PySCFString += f'for i in range(nstates):\\n'\n",
      "PySCFString += f'    if i == state:\\n'\n",
      "PySCFString += f'        writeData(f\"0 -> {i:<3}     {0.0:4.6f}    {0.0:4.6f}      {0.0:>7.2f}      {0.0:.6f}\")\\n'\n",
      "PySCFString += f'    else:\\n'\n",
      "PySCFString += f'        eEh = abs((mc_es.e_tot[i]+nevptEnergy[i]) - (mc_es.e_tot[state]+nevptEnergy[state]))\\n'\n",
      "PySCFString += f'        eEh = eEh\\n'\n",
      "PySCFString += f'        eeV = eEh * 27.2114\\n'\n",
      "PySCFString += f'        enm = evToNm(eeV)\\n'\n",
      "PySCFString += f'        dipole = np.linalg.norm(np.square(makedip(i)))\\n'\n",
