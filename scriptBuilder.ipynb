      "PySCFString += f'C    -2.7915438196    -0.0546000078     0.0000088890\\n'\n",
      "PySCFString += f'C    -1.9671308773    -1.1580673689    -0.0000239681\\n'\n",
      "PySCFString += f'C    -0.5764718046    -0.6994653673    -0.0000142451\\n'\n",
      "PySCFString += f'C     0.5728297294    -1.6187337425    -0.0000426176\\n'\n",
      "PySCFString += f'H     2.6370336944    -2.1323172911    -0.0000614692\\n'\n",
      "PySCFString += f'H     3.6607844414    -0.0038820359    -0.0000039511\\n'\n",
      "PySCFString += f'H     2.6896735030     2.1198515641     0.0000577232\\n'\n",
      "PySCFString += f'H     0.3510733896     2.7102434294     0.0000792260\\n'\n",
      "PySCFString += f'H    -2.2748732140     2.1849107023     0.0000707302\\n'\n",
      "PySCFString += f'H    -3.8723267585    -0.0402617537     0.0000117150\\n'\n",
      "PySCFString += f'H    -2.2668505376    -2.1940434715    -0.0000523940\\n'\n",
      "PySCFString += f'H     0.3219731626    -2.6732033772    -0.0000717168\\n'\n",
      "PySCFString += f'    \"\"\"\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'spin = 0\\n'\n",
      "PySCFString += f'state = 2\\n'\n",
      "PySCFString += f'nstates = 3\\n'\n",
      "PySCFString += f'norbs = 10\\n'\n",
      "PySCFString += f'nelec = 10\\n'\n",
      "PySCFString += f'sp_conv_e = 1e-10\\n'\n",
      "PySCFString += f'opt_conv_e = 1e-8\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'mol.charge = 0\\n'\n",
      "PySCFString += f'mol.spin = spin\\n'\n",
      "PySCFString += f'mol.basis = \"cc-pvdz\"\\n'\n",
      "PySCFString += f'mol.max_memory = 65536\\n'\n",
      "PySCFString += f'mol.verbose = 4\\n'\n",
      "PySCFString += f'mol.output = f\"{jobName}.out\"\\n'\n",
      "PySCFString += f'mol.build()\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'weights_ss = np.zeros(nstates)\\n'\n",
      "PySCFString += f'weights_ss[state] = 1\\n'\n",
      "PySCFString += f'weights_sa = np.ones(nstates)/nstates\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'# SS Opt Cycle\\n'\n",
      "PySCFString += f'mc_ss = mol.CASSCF(norbs, nelec).state_average(weights_ss)\\n'\n",
      "PySCFString += f'mc_ss.max_cycle_macro = 300\\n'\n",
      "PySCFString += f'mc_ss.max_cycle_micro = 20\\n'\n",
      "PySCFString += f'mc_ss.max_stepsize = 0.1\\n'\n",
      "PySCFString += f'mc_ss.conv_tol = opt_conv_e\\n'\n",
      "PySCFString += f'mc_ss.fix_spin_(ss=spin)\\n'\n",
      "PySCFString += f'new_mol = mc_ss.nuc_grad_method().as_scanner().optimizer().kernel()\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'with open(f\"{jobName}.xyz\", \"w+\") as f:\\n'\n",
      "PySCFString += f'    f.write(printXYZ(new_mol))\\n'\n",
      "PySCFString += f'\\n'\n",
      "PySCFString += f'# Starting point\\n'\n",
      "PySCFString += f'mf = new_mol.RHF()\\n'\n",
